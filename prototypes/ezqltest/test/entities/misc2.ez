temp_readings = stream (:room_id, :temperature);;
entries = stream(:room_id, :product_id);;

entity Room =
  createFrom(temp_readings, :room_id)
  hasMany :products;;


entity Product =
  createFrom (entries, :product_id)
  belongsTo :room;;




