temp_readings = stream (:room_id, :temperature);;

lastTemp = temp_readings.last(:temperature);;

c = let z = { a = { b = fun (t:int) -> t } } in
    (z.a.b)(lastTemp);;

when (c.updated(), fun ev -> print(c));;


