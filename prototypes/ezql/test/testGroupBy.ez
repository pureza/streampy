temp_readings = stream (:room_id, :temperature);

sumPerRoom = temp_readings
               .groupby(:room_id, g -> g.sum(:temperature));


sumPerRoomBiggerThan50 = sumPerRoom
                           .where(s -> s > 50);

sumPerRoomBiggerThan503secs = sumPerRoom
                                .where(s -> (s[3 sec] > 50).any());


